<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="Fluxos de aprovação diferentes" icon="sap-icon://simple-payment">
		<content>
			<Table items="{ path:'/OutrasPropostasSet' }">
				<columns>
					<Column >
						<Label text="Proposta"/>
					</Column>
					<Column width="20px">
						<Label text=""/>
					</Column>
					<Column>
						<Label text="Status"/>
					</Column>
					<Column>
						<Label text="Utilizador"/>
					</Column>
					<Column>
						<Label text="Comentários"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem type="Navigation" press="handlePressItem"></ColumnListItem>
				</items>
			</Table>
		</content>
		<endButton>
			<Button text="Fechar" icon="sap-icon://sys-cancel" press="onOutPropCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>